{
  "name": "project1-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "the-project/packages/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev:frontend": "npm run dev -w @theproject/frontend",
    "build:frontend": "npm run build -w @theproject/frontend",
    "dev": "cd the-project && npm run dev",
    "build": "cd the-project && npm run build",
    "test": "cd the-project && npm run test"
  }
}
